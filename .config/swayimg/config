[viewer]
loop = no
window = extend

[keys.viewer]
Shift+j = next_file
Shift+k = prev_file

h = step_left  10
j = step_down  10
k = step_up    10
l = step_right 10

# 1000 is the maximum, see move_image in viewer.c
g       = step_up   1000
Shift+g = step_down 1000

[keys.gallery]
h = step_left
j = step_down
k = step_up
l = step_right
